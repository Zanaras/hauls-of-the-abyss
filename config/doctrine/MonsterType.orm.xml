<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="App\Entity\MonsterType">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>

		<field name="name" type="string"/>
		<field name="size" type="string"/>
		<field name="neutral" type="boolean"/>
		<field name="alive" type="boolean"/>
        <field name="image" type="string" />
        <field name="imageDead" type="string" />
		<field name="attackType" type="array"/>

	</entity>
</doctrine-mapping>
