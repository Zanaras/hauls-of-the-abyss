<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="App\Entity\Race" table="race">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="size" type="string"/>
		<field name="agility" type="float"/>
		<field name="charisma" type="float"/>
		<field name="constitution" type="float"/>
		<field name="endurance" type="float"/>
		<field name="intelligence" type="float"/>
		<field name="spirit" type="float"/>
		<field name="perception" type="float"/>

	</entity>
</doctrine-mapping>
