<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping>
	<entity name="App\Entity\RoomType">
		<id name="id" type="integer">
			<generator strategy="IDENTITY"/>
		</id>
		<field name="name" type="string"/>
		<field name="spawn" type="boolean"/>
		<field name="teleporter" type="boolean"/>
		<field name="portal" type="boolean"/>
		<field name="allowUp" type="boolean"/>
		<field name="allowDown" type="boolean"/>
		<field name="modifiers" type="array"/>
		<field name="alternate" type="string" nullable="true"/>

	</entity>
</doctrine-mapping>
